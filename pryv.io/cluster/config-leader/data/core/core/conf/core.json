{
  "env": "production",
  "http": {
    "ip": "0.0.0.0",
    "port": "9000"
  },
  "database": {
    "host": "mongodb",
    "port": 27017,
    "name": "pryv-node"
  },
  "eventFiles": {
    "attachmentsDirPath": "/app/data/attachments",
    "previewsDirPath": "/app/data/previews"
  },
  "eventTypes": {
    "sourceURL": "EVENT_TYPES_URL"
  },
  "auth": {
    "trustedApps": "TRUSTED_APPS",
    "sessionMaxAge": SESSION_MAX_AGE,
    "ssoCookieDomain": ".DOMAIN",
    "ssoCookieSignSecret": "SSO_COOKIE_SIGN_SECRET",
    "filesReadTokenSecret": "FILES_READ_TOKEN_SECRET",
    "passwordResetRequestMaxAge": PASSWORD_RESET_MAX_AGE,
    "passwordResetPageURL": "https://sw.DOMAIN/access/reset-password.html",
    "adminAccessKey": "CORE_SYSTEM_KEY"
  },
  "services": {
    "register": {
      "url": "https://reg.DOMAIN",
      "key": "REGISTER_SYSTEM_KEY_1"
    },
    "email": {
      "enabled": EMAIL_ACTIVATION,
      "method": "microservice",
      "url": "https://mail.DOMAIN/sendmail/",
      "key": "CORE_MAIL_KEY",
      "welcomeTemplate": "welcome-email",
      "resetPasswordTemplate": "reset-password"
    },
    "reporting": {
      "licenseName": "REPORTING_LICENSE_NAME",
      "templateVersion": "REPORTING_TEMPLATE_VERSION"
    }
  },
  "updates": {
    "ignoreProtectedFields": UPDATES_IGNORE_PROTECTED_FIELDS
  },
  "audit": VERSIONING_SETTINGS,
  "webhooks": WEBOOKS_SETTINGS,
  "logs": {
    "console": {
      "active": true,
      "level": "info",
      "colorize": true
    },
    "airbrake": {
      "active": false,
      "key": "",
      "projectId": ""
    }
  }
}